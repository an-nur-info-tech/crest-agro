RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP:X-Forwarded-SSL} !on
RewriteCond %{HTTP_HOST} ^crest\-agro\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.crest\-agro\.com$
RewriteRule ^(.*)$ "https\:\/\/crest\-agro\.com\/$1" [R=301,L]

